    key <AB03> {	[	  c,	C		]	};
    key <AB04> {	[	  v,	V		]	};
    key <AB05> {	[     comma,	less		]	};
    key <AB06> {	[    period,	greater		]	};
    key <AB07> {	[	  b,	B		]	};
    key <AB08> {	[	  n,	N		]	};
    key <AB09> {	[	  m,	M		]	};
    key <AB10> {	[     slash,	question	]	};

    key <BKSL> {	[ semicolon,	colon		]	};

