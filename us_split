    key <AE10> {	[	  8,	asterisk	]	};
    key <AE11> {	[	  9,	parenleft	]	};
    key <AE12> {	[	  0,	parenright	]	};

    key <AD01> {	[	  q,	Q 		]	};
    key <AD02> {	[	  w,	W		]	};
    key <AD03> {	[	  e,	E		]	};
    key <AD04> {	[	  r,	R		]	};
    key <AD05> {	[	  t,	T		]	};
    key <AD06> {	[ bracketleft,	braceleft	]	};
    key <AD07> {	[ bracketright,	braceright	]	};
    key <AD08> {	[	  y,	Y		]	};
    key <AD09> {	[	  u,	U		]	};
    key <AD10> {	[	  i,	I		]	};
    key <AD11> {	[	  o,	O		]	};
    key <AD12> {	[	  p,	P		]	};

    key <AC01> {	[	  a,	A 		]	};
    key <AC02> {	[	  s,	S		]	};
    key <AC03> {	[	  d,	D		]	};
    key <AC04> {	[	  f,	F		]	};
    key <AC05> {	[	  g,	G		]	};
    key <AC06> {	[ backslash,         bar	]	};
    key <AC07> {	[ apostrophe,	quotedbl	]	};
